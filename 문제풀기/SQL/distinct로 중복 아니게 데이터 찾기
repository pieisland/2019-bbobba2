#최댓값 구하기. 단일행 서브 쿼리 이용.
SELECT DATETIME
FROM ANIMAL_INS
WHERE DATETIME = (SELECT MAX(DATETIME) FROM ANIMAL_INS)

..사실 그냥
SELECT MAX(DATETIME)
FROM ANIMA_INS
하면 된다.


#COUNT 함수로 튜플 수 세기
SELECT COUNT(*) AS count
FROM ANIMAL_INS


#DISTINCT를 이용해 중복되지 않고 NULL이 아닌 이름의 수 세기
SELECT COUNT(DISTINCT NAME) AS count
FROM ANIMAL_INS
WHERE NAME IS NOT NULL
