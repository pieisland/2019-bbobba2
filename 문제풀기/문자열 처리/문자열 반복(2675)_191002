#include <iostream>
#include <string>
#include <cstdio>
#include <vector>
using namespace std;

int main() {

	int n;
	cin >> n;
	while (n--) {
		int num;
		string s;
		vector<char> v;
		cin >> num >> s;
		for (int i = 0; i < s.size(); i++) {
			v.push_back(s[i]);
		}
		for (int i = 0; i < v.size(); i++) {
			for (int j = 0; j < num; j++) {
				cout << v[i];
			}
		}
		cout << endl;
	}

	return 0;
}
