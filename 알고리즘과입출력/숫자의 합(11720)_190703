int main(){
	
	cin.tie(NULL);
	int n;
	cin >> n;

	int num;
	int sum = 0;
	for (int i = 0; i < n; i++)
	{
		scanf("%1d", &num);
		sum += num;
	}

	cout << sum << endl;

	return 0;
}
